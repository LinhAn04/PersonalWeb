<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1"
         xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
        http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <!--Survey-->
    <servlet>
        <servlet-name>SurveyList</servlet-name>
        <servlet-class>Survey.surveyform.SurveyList</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SurveyList</servlet-name>
        <url-pattern>/emailList</url-pattern>
    </servlet-mapping>
    <!--Ch05ex1-->
    <servlet>
        <servlet-name>Ch05ex1</servlet-name>
        <servlet-class>Ch05.ex1.Ch05ex1Servlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Ch05ex1</servlet-name>
        <url-pattern>/Ch05ex1</url-pattern>
    </servlet-mapping>
    <!--Ch05ex2-->
    <servlet>
        <servlet-name>Ch05ex2</servlet-name>
        <servlet-class>Ch05.ex2.Ch05ex2Servlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Ch05ex2</servlet-name>
        <url-pattern>/Ch05ex2</url-pattern>
    </servlet-mapping>


    <session-config>
        <session-timeout>30</session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>Web/webView.html</welcome-file>
    </welcome-file-list>

</web-app>